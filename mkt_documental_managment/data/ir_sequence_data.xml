<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="documental_requirement_sequence" model="ir.sequence">
        <field name="name">Requirement</field>
        <field name="code">documental.requirements</field>
        <field name="active">TRUE</field>
        <field name="prefix">RQ-%(year)s-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>

    <record id="documental_settlements_sequence" model="ir.sequence">
        <field name="name">Settlements</field>
        <field name="code">documental.settlements</field>
        <field name="active">TRUE</field>
        <field name="prefix">FL-%(year)s-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>

    <!-- Secuencia para Lima -->
    <record id="documental_mobility_sequence" model="ir.sequence">
        <field name="name">Mobility</field>
        <field name="code">documental.mobility.expediture</field>
        <field name="active">TRUE</field>
        <field name="prefix">PM-%(year)s-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>

    <!-- Secuencia para Provincias -->
    <record id="documental_mobility_sequence_ciudad" model="ir.sequence">
        <field name="name">Movilidad ciudad</field>
        <field name="code">documental.mobility.expediture.ciudad</field>
        <!-- <field name="prefix">PM-%(year)s-%(provincia_code)s-</field> -->
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>

    <record id="budget_sequence" model="ir.sequence">
        <field name="name">Budget Control</field>
        <field name="code">budget</field>
        <field name="active">TRUE</field>
        <field name="prefix">PPTO%(year)s%(month)s-</field>
        <field name="padding">5</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>

    <record id="voucher_sequence" model="ir.sequence">
        <field name="name">Voucher</field>
        <field name="code">voucher</field>
        <field name="active">TRUE</field>
        <field name="prefix">V</field>
        <field name="padding">7</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="budget_modify_sequence" model="ir.sequence">
        <field name="name">Budget modify</field>
        <field name="code">budget.modify</field>
        <field name="active">TRUE</field>
        <field name="prefix">BM</field>
        <field name="padding">7</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
    </record>

    <record id="affidavit_sequence" model="ir.sequence">
        <field name="name">Affidavit</field>
        <field name="code">affidavit</field>
        <field name="active">TRUE</field>
        <field name="prefix">DJ</field>
        <field name="padding">6</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">TRUE</field>
    </record>


</odoo>

