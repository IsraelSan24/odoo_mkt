<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="view_photocheck_city_tree" model="ir.ui.view">
        <field name="name">photocheck.city.tree</field>
        <field name="model">photocheck.city</field>
        <field name="arch"  type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="view_photocheck_city_form" model="ir.ui.view">
        <field name="name">photocheck.city.form</field>
        <field name="model">photocheck.city</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_photocheck_city_action" model="ir.actions.act_window">
        <field name="name">Photocheck city</field>
        <field name="res_model">photocheck.city</field>
        <field name="view_mode">tree,form</field>    
    </record>
        
</odoo>
