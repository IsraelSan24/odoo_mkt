    <template id="gastos">
      <t t-call="website.layout">
        <div class="container">
         <form id="form_gastos" action="/guardar/gastos" method="post">
            <br/>
            <br/>
            <center>
              <h2>
                <b>PLANILLA DE GASTO POR MOVILIDAD</b>
              </h2>
            </center>
            <br/>
            <div class="card text-white bg-secondary mb-3" id="conter-primero-gastos">
              <div class="card-header">Datos del Documento</div>
              <div class="card-body" id="contenedor-gastos">
                <div class="conter-div-gastos">
                  <label for="exampleFormControlInput7" style="font-size:14px;">N° de PPTO :</label>
                  <input type="text" class="form-control" id="ppto" name="ppto" autocomplete="off"/>
                  <label for="exampleFormControlInput3" style="font-size:14px;">Periodo :</label>
                  <input type="text" class="form-control" id="periodo" name="periodo" autocomplete="off"/>
                </div>
                <div class="conter-div-gastos">
                  <label for="exampleFormControlInput8" style="font-size:14px;">Centro Costo :</label>
                  <input type="text" class="form-control" id="costo" name="costo" autocomplete="off"/>
                </div>
              </div>
            </div>

            <br/>
