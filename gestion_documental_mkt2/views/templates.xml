                  <input type="text" name="fecha_importe2" id="fecha_importe2" style="display:none;"/>
                  <input type="text" name="importe_total_fecha2" id="importe_total_fecha2" style="display:none;"/>
                  <input type="text" name="fecha_importe3" id="fecha_importe3" style="display:none;"/>
                  <input type="text" name="importe_total_fecha3" id="importe_total_fecha3" style="display:none;"/>
                  <input type="text" name="fecha_importe4" id="fecha_importe4" style="display:none;"/>
