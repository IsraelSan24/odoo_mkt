<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template id="requerimientos">
      <t t-call="website.layout">
        <div class="container">
          <title>Requerimientos</title>
          <form action="/guardar/requerimiento" method="post">
            <div style="display:flex;">
              <div style="width: -webkit-fill-available;margin:auto;" class="form-group form-field o_website_form_required_custom">
                  <br/>
                  <br/>
                  <p>
                    <h2>
                        <b>Requerimientos</b>
                    </h2>
                  </p>
                  <br/>
                  <div class="col-lg-7 col-md-8">
                      <input type="text" id="fecha_solicitud" name="fecha_solicitud" style="display:none;"/>
                      <input type="number" id="idnumeric_requerimiento" name="idnumeric_requerimiento"  style="display:none;"/>
                  </div>
                  <div class="card text-white bg-secondary mb-3" id="card-requerimiento-fase1">
                    <div class="card-header">Datos del Proveedor</div>
                    <div class="card-body" id="card-body-requerimiento">
                      <div id="card1-body-requerimiento-div">
                        <label class="col-md-12 col-sm-12 control-label" for="banco">Banco</label>
                        <div class="col-lg-12 col-md-12" id="contenedor1-requerimiento">
                          <select id="campo1-requerimiento" class="browser-default custom-select" name="banco">
                              <option value="Cuenta Registrada">Cuenta Registrada</option>
                              <option value="Banco de Comercio">Banco de Comercio</option>
                              <option value="Banco de Crédito del Perú">Banco de Crédito del Perú</option>
                              <option value="Banco Interamericano de Finanzas (BanBif)">Banco Interamericano de Finanzas (BanBif)</option>
                              <option value="Banco Pichincha">Banco Pichincha</option>
                              <option value="BBVA">BBVA</option>
                              <option value="Interbank">Interbank</option>
                              <option value="MiBanco">MiBanco</option>
                              <option value="Scotiabank Perú">Scotiabank Perú</option>
                              <option value="Banco GNB Perú">Banco GNB Perú</option>
                              <option value="Banco Ripley">Banco Ripley</option>
                              <option value="Banco Santander Perú">Banco Santander Perú</option>
                          </select>
                        </div>
                      <!-- <br/>-->  
                        <label class="col-md-12 col-sm-12 control-label" for="ncta">N. de Cta. Cte.</label>
                        <div class="col-lg-12 col-md-12" id="contenedor2-requerimiento">
                          <input type="text" class="form-control" name="ncta" autocomplete="off"/>
                        </div>
                      </div>
                      <div style="margin: auto;">
                        <label class="col-md-12 col-sm-12 control-label" for="Girado">Girado a</label>
                        <div class="col-lg-12 col-md-12" id="contenedor3-requerimiento">
                          <input type="text" class="form-control" name="Girado" autocomplete="off"/>
                        </div>
                        <!-- <br/>-->  
                        <label class="col-md-12 col-sm-12 control-label" for="suma">DNI o RUC</label>
                        <div class="col-lg-12 col-md-12" id="contenedor4-requerimiento">
                          <input type="number" class="form-control" name="suma" autocomplete="off"/>
                        </div>
                      </div>
                    </div>
                  </div>
                 
              </div>
            </div>
            <br/>
            <div class="card text-white bg-secondary mb-3" id="card-requerimiento-fase2">
              <div class="card-header">Datos del Requerimiento</div>
              <div class="card-body" id="card-requerimiento-fase2-body">
                <div class="card-requerimiento-fase2-div">
                  <label class="col-md-12 col-sm-12 control-label" for="cliente">Cliente</label>
                  <div class="col-md-12 col-sm-12" id="card-requerimiento-fase2-cliente">
                      <input type="text" class="form-control" name="cliente" autocomplete="off"/>
                  </div>
                  <label class="col-md-12 col-sm-12 control-label" for="actividad">Actividad</label>
                  <div class="col-md-12 col-sm-12"  id="card-requerimiento-fase2-actividad">
                      <input type="text" class="form-control" rows="5" name="actividad" autocomplete="off"/>
                  </div>
                  <!-- New field view -->
                  <label class="col-md-12 col-sm-12 control-label" for="importe_s_char">Por la Suma de</label>
                  <div class="col-md-12 col-sm-12"  id="card-requerimiento-fase2-actividad">
                      <input type="text" class="form-control" name="importe_s_char" autocomplete="off"/>
                  </div>
                 
                </div>
                <div style="margin: auto;">
                  <label class="col-md-12 col-sm-12 control-label" for="ppto">N. de PPTO</label>
                  <div class="col-md-12 col-sm-12">
                      <input type="text" class="form-control" name="ppto" autocomplete="off"/>
                  </div>
                  <label class="col-md-12 col-sm-12 control-label" for="centro_costo">Centro de Costo</label>
                  <div class="col-md-12 col-sm-12">
                      <input type="text" class="form-control" name="centro_costo" autocomplete="off"/>
                  </div>
                  <label class="col-md-12 col-sm-12 control-label" for="importe_s">Importe (S/)</label>
                  <div class="col-md-12 col-sm-12">
                      <input type="text" class="form-control decimales" name="importe_s" autocomplete="off"/>
                  </div>
                </div>
                <div style="margin: auto;">
                  <label class="col-md-12 col-sm-12 control-label" for="importe_uss">Importe (USS)</label>
                  <div class="col-md-12 col-sm-12">
                      <input type="text" class="form-control decimales" name="importe_uss" autocomplete="off"/>
                  </div>
                  <label class="col-md-12 col-sm-12 control-label" for="importe_cheque">N. de Cheque</label>
                  <div class="col-md-12 col-sm-12"> 
                      <input type="text" class="form-control decimales" name="importe_cheque" autocomplete="off"/>
                  </div>
                  <label class="col-md-12 col-sm-12 control-label" for="operacion">N. de Operación</label>
                  <div class="col-md-12 col-sm-12">
                      <input type="text" class="form-control decimales"  name="operacion" autocomplete="off"/>
                  </div>
                </div>
              </div>
              <div class="card-body" id="card-boy-concepto">
                <label class="col-md-12 col-sm-12 control-label" for="concepto">Concepto</label>
                <div class="col-lg-12 col-md-12" style="width: 100%;height: 150px;">
                    <textarea class="form-control" rows="5" name="concepto" autocomplete="off"></textarea>
                
                </div>
              </div>
            </div>
            <br/>
            <div class="card text-white bg-secondary mb-3" style="height:250px;background-color: #555555 !important;">
              <div class="card-header">Datos del Responsable</div>
              <div class="card-body">
                <label class="col-md-3 col-sm-4 control-label" style="font-size:15px;" for="responsable">Apellidos y Nombres : </label>
                <div class="col-lg-7 col-md-10">
                    <input type="text" class="form-control"  name="responsable" autocomplete="off"/>
                </div>
                <label class="col-md-3 col-sm-4 control-label" style="font-size:15px;" for="dni_responsable">DNI : </label>
                <div class="col-lg-7 col-md-10">
                    <input type="number" class="form-control"  name="dni_responsable" autocomplete="off"/>
                </div>
              </div>
            </div>
            <br/>
            <div class="card text-white bg-secondary mb-3" style="height:250px;background-color: #555555 !important;">
              <div class="card-header">Detalle</div>
              <div class="card-body">
                <textarea class="form-control" rows="5" name="detalle_responsable" autocomplete="off"></textarea>
              </div>
            </div>
            <br/>
            <br/>
            <div class="form-group" style="display:flex;">
              <div class="col-md-4">
                <button style="width:150px;height:50px;" data-toggle="modal" data-target="#exampleModalCenter" class="btn btn-danger">Guardar y Enviar</button>
              </div>
            </div>
            <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
              <div class="modal-dialog modal-dialog-centered" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Añadir Destinatario</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    </button>
                  </div>
                  <div class="modal-body">
                    <label class="col-md-3 col-sm-4 control-label" style="font-size:15px;" for="responsable">Correo : </label>
                    <div class="col-lg-7 col-md-10">
                        <input id="correos_requerimientos" name="correos_requerimientos" type="email"  class="form-control" autocomplete="off" required="true"/>
                    </div>
                  </div>
                  <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancelar</button>
                    <a href='/guardar/requerimiento'> 
                      <button id="enviar_requerimiento" name="enviar_requerimiento" type="submit" class="btn btn-primary">Enviar y Guardar</button>
                    </a>
                  </div>
                </div>
              </div>
            </div>
            <br/>
            <br/>
          </form>
        </div>
      </t>
    </template>

