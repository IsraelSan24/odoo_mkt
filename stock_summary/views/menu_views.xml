<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <menuitem id="stock_summary_root" name="Stock Summary" web_icon="stock_summary,static/description/stock_summary_icon.png"/>
        <menuitem id="stock_summary_menu" name="Report View" action="view_stock_summary_action" parent="stock_summary_root" sequence="0"/>
        <menuitem id="stock_summary_in_menu" name="In" action="view_stock_summary_in_action" parent="stock_summary_root" sequence="1"/>
        <menuitem id="stock_summary_out_menu" name="Out" action="view_stock_summary_out_action" parent="stock_summary_root" sequence="2"/>
        <menuitem id="stock_summary_report_menu" name="Report" parent="stock_summary_root" sequence="3"/>
            <menuitem id="stock_report_submenu" name="Report" action="action_stock_summary_report_view" parent="stock_summary_report_menu" sequence="0"/>

    <record model='ir.actions.act_url' id="action_to_portal">
        <field name="name">Portal</field>
        <field name="url">https://odoo.marketing-alterno.com/</field>
    </record>
    <menuitem id="menu_stock_to_portal" name="Portal" action="action_to_portal" parent="stock_summary_root" sequence="4"/>

</odoo>
