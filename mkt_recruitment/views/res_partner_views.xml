<odoo>

    <record id="view_res_partner_reference_location_form" model="ir.ui.view">
        <field name="name">res.partner.reference.location.form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <field name="website" position="before">
                <field name="personal_email"/>
            </field>
            <field name="vat" position="before">
                <field name="reference_location"/>
                <field name="belong_applicant_id"/>
            </field>
            <xpath expr="//page[@name='internal_notes']" position="after">
                <page name="private_information" string="Private Information"
                    groups="hr_contract.group_hr_contract_manager,hr_recruitment.group_hr_recruitment_manager">
                    <group>
                        <group string="Basic information">
                            <field name="birthday"/>
                            <field name="gender"/>
                            <field name="age"/>
                            <field name="nationality_id" class="oe_edit_only"/>
                            <field name="demonym"/>
                        </group>
                        <group string="Emergency information">
                            <button string="Send documents" name="button_send_documents" type="object" class="oe_highlight" group="hr_contract.group_hr_contract_manager"/>
			                <button string="Actualizar informacion" name="buton_update_applicant_partner" type="object" class="oe_highlight" group="hr_contract.group_hr_contract_manager"/>
                            <field name="is_validate" widget="boolean_toggle" group="hr_contract.group_hr_contract_manager"/>
                            <field name="emergency_contact"/>
                            <field name="emergency_phone" widget="phone"/>
                            <field name="emergency_contact_relationship"/>
                        </group>
                    </group>
                    <group string="Marital information">
                        <group>
                            <field name="marital"/>
                        </group>
                        <group>
                            <field name="children"/>
                        </group>
                    </group>
                    <group string="Employment relationship data">
                        <group>
                            <field name="spreadsheet"/>
                            <field name="private_pension_system"/>
                            <field name="afp_first_job" attrs="{'invisible':[('private_pension_system','=',False)]}"/>
                            <field name="coming_from_onp" attrs="{'invisible':[('private_pension_system','=',False)]}"/>
                            <field name="coming_from_afp" attrs="{'invisible':[('private_pension_system','=',False)]}"/>
                        </group>
                        <group>
                            <field name="national_pension_system"/>
                        </group>
                    </group>
                    <group string="Education">
                        <field name="education_level"/>
                        <field name="education_start_date"/>
                        <field name="education_end_date"/>
                        <field name="institution"/>
                        <field name="profession"/>
                    </group>
                    <group string="Documents">
                        <group>
                            <field name="document_okay" widget="boolean_toggle" groups="hr_contract.group_hr_contract_manager"/>
                        </group>
                        <group>
                            <button string="Update documents" name="attach_applicant_files" type="object" class="oe_highlight"/>
