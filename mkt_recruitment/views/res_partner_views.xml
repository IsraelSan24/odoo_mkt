        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <tree
                decoration-success="document_okay == True and children_okay == True">
                <header>
                    <button string="Download documents" name="download_attach_files" type="object" class="oe_highlight"/>
                </header>
                <field name="name"/>
                <field name="is_current_dni"/>
                <field name="is_current_dni_back"/>
                <field name="is_services_receipt"/>
                <field name="is_certijoven"/>
                <field name="certificate_of_vaccination" invisible="True"/> 
                <field name="health_card" invisible="True"/> 
                <field name="contributions_report" invisible="True"/> 
                <field name="is_electronic_fine" invisible="True"/> 
                <field name="is_t_record"/>
                <field name="document_okay" widget="boolean_toggle" readonly="True"/>
                <field name="children_okay" widget="boolean_toggle" readonly="True"/>
            </tree>
        </field>
    </record>

